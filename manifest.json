{
  "name": "ASCIIize",
  "description": "Turn any image to ASCII art. Kindoff",
  "version": "0.0.2",
  "icons": {
    "128": "assets/icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "reload.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "ASCIIize",
    "default_icon": {
      "128": "assets/icon.png"
    },
    "default_popup": "views/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "client.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "assets/icon.png",
    "assets/icon_active.png",
    "worker.js"
  ]
}
