{
  "name": "Asciiize",
  "description": "Turn any image to ASCII art. Kindoff",
  "version": "0.0.1",
  "icons": {
    "16": "assets/icon_16x16.png",
    "32": "assets/icon_32x32.png",
    "48": "assets/icon_48x48.png",
    "128": "assets/icon_128x128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "common.js",
      "reload.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Simple example.",
    "default_icon": {
      "19": "assets/icon_19x19.png",
      "38": "assets/icon_38x38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "common.js",
        "client.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "assets/icon_38x38.png",
    "assets/icon_19x19.png"
  ]
}
