{
  "name": "ASCIIize",
  "description": "Turn any image to ASCII art. Kindoff",
  "version": "0.0.1",
  "icons": {
    "128": "assets/icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "storage",
    "contextMenus",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "ASCIIize",
    "default_icon": {
      "128": "assets/icon.png"
    },
    "default_popup": "views/popup.html"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "assets/icon.png",
    "assets/icon_active.png",
    "worker.js"
  ]
}
